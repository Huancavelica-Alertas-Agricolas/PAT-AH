{
  "name": "rest-service",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "start:prod": "node src/main.js",
    "test": "jest --config jest.config.json",
    "test:unit": "jest --config jest.config.json --runInBand",
    "test:cov": "jest --config jest.config.json --coverage --runInBand",
    "test:integration:docker:ps1": "powershell -ExecutionPolicy Bypass -File test/setup/run_tests_with_postgres.ps1",
    "test:integration:docker:sh": "bash test/setup/run_tests_with_postgres.sh"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/microservices": "^11.0.6",
    "@nestjs/platform-express": "^11.1.9",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.3",
    "graphql": "^16.12.0",
    "graphql-subscriptions": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "twilio": "^5.10.6"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.9",
    "jest": "^29.0.0",
    "node-fetch": "^2.7.0",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "@as-integrations/express5": "^1.0.0"
  }
